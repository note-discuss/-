<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.note"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="21" />
    <instrumentation android:name="android.test.InstrumentationTestRunner" android:targetPackage="cn.csdn.activity"></instrumentation>

   <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:label="@string/app_name"
            android:name=".LoginActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".RegisterActivity"></activity>
        <activity 
            android:name=".ClassNoteActivity"></activity>
        <uses-library android:name="android.test.runner"/>
        <activity 
            android:name=".AddTopicActivity"></activity>
        <uses-library android:name="android.test.runner"/>
        <activity 
            android:name=".UserInfoActivity"></activity>
        <uses-library android:name="android.test.runner"/>
        <activity 
            android:name="NoteListActivity"></activity>
        <uses-library android:name="android.test.runner"/>
        <activity 
            android:name="AddNoteActivity"></activity>
        <uses-library android:name="android.test.runner"/>
        <activity 
            android:name="NoteItemActivity"></activity>
        <uses-library android:name="android.test.runner"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET" /> 
</manifest>
